<coba>:
	size_hint:1,1
	background_color:
	canvas:
		Color:
			rgba:self.background_color
		Rectangle:
			pos:root.x,root.height/10
			size:root.width,root.height*9/10
	Button:
		pos:root.x,root.y
		size:root.width/3,root.height/10
		text:'Submit'
		on_press:root.render2()
	TextInput:
		id:ti
		pos:root.width/3,0
		size:root.width*2/3,root.height/10